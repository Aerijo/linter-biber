{
  "name": "linter-biber",
  "version": "0.0.2",
  "description": "Bibliography linter for biber engine",
  "main": "lib/main.js",
  "repository": "https://github.com/Aerijo/linter-biber",
  "bugs": "https://github.com/Aerijo/linter-biber/issues",
  "keywords": [
    "linter",
    "biber",
    "latex",
    "biblatex"
  ],
  "author": "Benjamin Gray",
  "license": "MIT",
  "dependencies": {
    "tree-sitter": "latest",
    "tree-sitter-biber": "latest"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    }
  },
  "configSchema": {
    "ignoredLints": {
      "type": "array",
      "default": [
        "14",
        "1",
        "16",
        "7",
        "5"
      ],
      "items": {
        "type": "string"
      }
    },
    "maxNumberOfLines": {
      "type": "integer",
      "default": 10000,
      "description": "Use this to customise roughly how many lines will be linted before giving up. Restart required to apply changes"
    }
  },
  "devDependencies": {
    "eslint": "^5.0.1"
  }
}
